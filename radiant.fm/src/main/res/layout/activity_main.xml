<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="fm.radiant.android.activities.MainActivity">

    <TextView
        android:text="@string/hello_world"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <Button
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:text="start"
        android:id="@+id/button_login2"
        android:onClick="logout2"
        android:enabled="true"
        android:clickable="true"
        android:layout_marginTop="100dp"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        />
    <Button
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:text="stop"
        android:id="@+id/button_login3"
        android:onClick="logout3"
        android:enabled="true"
        android:clickable="true"
        android:layout_marginTop="180dp"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        />


    <Button
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:text="play"
        android:id="@+id/button_login4"
        android:onClick="play"
        android:enabled="true"
        android:clickable="true"
        android:layout_marginTop="240dp"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:text="pause"
        android:id="@+id/button_login5"
        android:onClick="pause"
        android:enabled="true"
        android:clickable="true"
        android:layout_marginTop="300dp"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        />


    <Button
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:text="@string/button_login"
        android:id="@+id/button_login"
        android:onClick="logout"
        android:enabled="true"
        android:clickable="true"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        />

</RelativeLayout>
