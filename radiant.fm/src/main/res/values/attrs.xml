<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgressBar">
        <attr name="progress"                 format="integer" />
        <attr name="max"                      format="integer" />
        <attr name="progress_backgroundWidth" format="dimension" />
        <attr name="progress_foregroundWidth" format="dimension" />
        <attr name="progress_backgroundColor" format="color" />
        <attr name="progress_foregroundColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ImageSquareButton">
        <attr name="drawableCenter" format="integer" />
    </declare-styleable>

    <attr name="circleProgressBarStyle" format="reference" />
    <attr name="imageSquareButtonStyle" format="reference" />

</resources>